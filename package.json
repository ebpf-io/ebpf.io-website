{
  "name": "ebpf-gatsby",
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Alex Barashkov <alex@pixelpoint.io>",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.6.0"
  },
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write --ignore-path .gitignore .",
    "lint": "npm run lint:js & npm run lint:md",
    "lint:fix": "npm run lint:js:fix & npm run lint:md:fix",
    "lint:js": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint:js:fix": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --fix --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/react": "^2.3.0",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^3.6.0",
    "framer-motion": "^10.18.0",
    "gatsby": "^5.16.1",
    "gatsby-plugin-canonical-urls": "^5.16.0",
    "gatsby-plugin-feed": "^5.16.0",
    "gatsby-plugin-google-gtag": "^5.16.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-image": "^3.16.0",
    "gatsby-plugin-mdx": "^5.16.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-react-i18next": "^3.0.1",
    "gatsby-plugin-twitter": "^5.16.0",
    "gatsby-remark-copy-linked-files": "^6.16.0",
    "gatsby-remark-images": "^7.16.0",
    "gatsby-remark-responsive-iframe": "^6.16.0",
    "gatsby-remark-video": "^1.2.5",
    "lodash.get": "^4.4.2",
    "lodash.shuffle": "^4.2.0",
    "markdown-it": "^14.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-modal": "^3.16.3",
    "react-paginate": "^8.3.0",
    "react-player": "^2.16.1",
    "react-share": "^5.2.2",
    "react-syntax-highlighter": "^16.1.0",
    "react-use": "^17.6.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^1.0.0",
    "swiper": "^12.1.2",
    "tailwindcss": "^3.4.19"
