{
  "name": "ebpf-gatsby",
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Alex Barashkov <alex@pixelpoint.io>",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.6.0"
  },
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write --ignore-path .gitignore .",
    "lint": "npm run lint:js & npm run lint:md",
    "lint:fix": "npm run lint:js:fix & npm run lint:md:fix",
    "lint:js": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint:js:fix": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --fix --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/react": "^2.2.1",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.29.3",
    "framer-motion": "^7.6.19",
    "gatsby": "^5.10.0",
    "gatsby-plugin-canonical-urls": "^5.5.0",
    "gatsby-plugin-feed": "^5.5.0",
    "gatsby-plugin-google-analytics": "^5.5.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-image": "^3.5.0",
    "gatsby-plugin-mdx": "^5.5.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-twitter": "^5.5.0",
    "gatsby-remark-copy-linked-files": "^6.5.0",
    "gatsby-remark-images": "^7.5.0",
    "gatsby-remark-responsive-iframe": "^6.5.0",
    "gatsby-remark-video": "^1.2.5",
    "lodash.get": "^4.4.2",
    "markdown-it": "^13.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.1",
    "react-paginate": "^8.1.4",
    "react-player": "^2.11.0",
    "react-share": "^4.4.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^1.0.0",
    "swiper": "^8.4.7",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-alias-imports": "^1.0.6",
    "gatsby-plugin-manifest": "^5.5.0",
    "gatsby-plugin-postcss": "^6.5.0",
    "gatsby-plugin-sharp": "^5.8.1",
    "gatsby-plugin-sitemap": "^6.5.0",
    "gatsby-plugin-svgr-svgo": "^2.0.0",
    "gatsby-source-filesystem": "^5.5.0",
    "gatsby-transformer-sharp": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss-safe-area": "^0.2.2"
  },
  "keywords": [
    "react",
    "gatsby",
    "tailwind"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixel-point/gatsby-tailwind-starter"
  },
  "bugs": {
    "url": "https://github.com/pixel-point/gatsby-tailwind-starter/issues"
  }
}
